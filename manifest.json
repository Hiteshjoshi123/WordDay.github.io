{
    "manifest_version":2,
    "name":"Word Of The Day",
     "version":"1.0",
     "description":"Get New Word Every Day",
     "icons":{
         "128":"icon128.png",
         "48":"icon48.png",
         "16":"icon16.png"
     },
     "browser_action":{
        "default_icon":"icon16.png",
        "default_popup":"popup.html"
     },
     "permissions":[
         "storage",
         "background",
         "tabs",
         "contextMenus"
     ]
}